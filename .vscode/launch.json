    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run from Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SK.ChatBot.__Console/bin/Debug/netcoreapp3.1/SK.ChatBot.Console.dll",
            "args": [],
            "env": {"openweathermapkey": ""},
            "cwd": "${workspaceRoot}",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}